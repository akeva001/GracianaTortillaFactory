{
  "name": "gracianatortillafactory",
  "productName": "Graciana Tortilla Factory",
  "version": "1.0.0",
  "private": true,
  "author": "Alex Kevakian",
  "description": "Graciana Tortilla Factory",
  "dependencies": {
    "@ramonak/react-excel": "^1.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-bootstrap-dropdown": "^0.3.0",
    "react-bootstrap-form": "^0.1.4-beta6",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-dom": "^17.0.2",
    "react-dropdown": "^1.9.2",
    "react-scripts": "4.0.3",
    "react-select": "^5.2.1",
    "react-to-print": "^2.14.0",
    "web-vitals": "^1.0.1",
    "xlsx": "^0.17.4"
  },
  "GTF": "GTF",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "electron:package:mac": "npm run-script build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "com.electron.gracianatortillafactory",
    "productName": "Graciana Tortilla Factory",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public/electron.js"
    },
    "mac": {
      "icon": "build/logo.icns",
      "target": "dmg"
    },
    "win": {
      "icon": "build/logo.ico",
      "target": "@testing-library/jest-dom"
    },
    "linux": {
      "icon": "build/icon.png",
      "target": "deb"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^15.3.2",
    "electron-builder": "^22.14.5",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^15.4.0",
    "electronmon": "^2.0.2",
    "wait-on": "^6.0.0"
  }
}
